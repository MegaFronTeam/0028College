mixin sCard(data)
	// start sCard
	+b.SECTION.sCard.section#sCard&attributes(attributes)
		.container
			.row
				mixin item(t, p)
					.col-md-6 
						+e.item
							+e.title= t
							p= p
							+e.btn(href='#') Подробнее
				+item(`Специальности после 9 класса`,`Лучший способ быстрее обучиться и начать зарабатывать раньше сверстников!`)
				+item(`Специальности после 9 класса`,`Лучший способ быстрее обучиться и начать зарабатывать раньше сверстников!`)
				mixin item2(i, t, p,tel)
					- let th =  tel.trim()
					.col-md-6 
						+e.item--2
							+pic(`iframe-${i}.png`,'',0)
							//- img(src=``, alt="")
							+e.titler!= t
							p= p
							+e.tel(href=`tel:${th}`)
								+icon('tel')
								= tel
				+item2(`1`,`ЦЕНТР  ПРОМЫШЛЕННЫХ ТЕХНОЛОГИЙ`,`г.Москва, ул. Бобруйская д. 7`,`8 (499) 673-01-41`)
				+item2(`2`,`ЦЕНТР  ТЕХНОЛОГИЙ ТРАНСПОРТА`,`г.Москва, ул. Бобруйская д. 7`,`‭8 (495) 441-90-90`)
				+item2(`3`,`ЦЕНТР ГРАДОСТРОИТЕЛЬНЫХ ТЕХНОЛОГИЙ`,`г.Москва, ул. Бобруйская д. 7`,`8 (495) 934-03-22`)
				+item2(`4`,`ТЕХНОПАРК <br>  ВНУКОВО`,`г.Москва, ул. Дивизионная д. 4`,`8 (495) 736-04-90`)
				//- +item(``,``)
	// end sCard