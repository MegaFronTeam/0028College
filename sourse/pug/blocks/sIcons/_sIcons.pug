mixin sIcons(data)
	// start sIcons
	+b.SECTION.sIcons.section#sIcons&attributes(attributes)
		.container
			.row
				.col-auto
					img(src="img/svg/foot-icon-1.svg", alt="")
					p Отсрочка <br> от Армии
					
				.col-auto
					img(src="img/svg/foot-icon-2.svg", alt="")
					p Диплом государственного <br> образца
				.col-auto
					img(src="img/svg/foot-icon-3.svg", alt="")
					p Поступление  <br> без Экзаменов и ЕГЭ

	// end sIcons